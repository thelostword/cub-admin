{
  "name": "cub-admin",
  "version": "0.0.0-alpha.5",
  "description": "中后台系统基础库",
  "keywords": [
    "cub-admin",
    "cub-admin-plus",
    "vue-admin",
    "admin",
    "vue",
    "element-plus"
  ],
  "homepage": "https://github.com/thelostword/cub-admin",
  "bugs": {},
  "license": "MIT",
  "author": {
    "name": "losting",
    "email": "thelostword@163.com",
    "url": "https://github.com/thelostword"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/thelostword/cub-admin.git"
  },
  "files": [
    "packages.json",
    "README.md",
    "LICENSE",
    "dist"
  ],
  "type": "module",
  "main": "./dist/core.cjs",
  "module": "./dist/core.js",
  "types": "./dist/core.d.ts",
  "exports": {
    ".": {
      "types": "./dist/core.d.ts",
      "import": "./dist/core.js",
      "require": "./dist/core.cjs"
    },
    "./*": "./*"
  },
  "packageManager": "pnpm@8.6.0",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky install",
    "dev": "pnpm preview dev",
    "build": "vue-tsc && vite build",
    "components": "pnpm --filter=@cub-admin/components",
    "preview": "pnpm --filter=@cub-admin/preview",
    "commit": "git-cz",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "dependencies": {
    "color": "^4.2.3",
    "lodash-es": "^4.17.21"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@commitlint/cz-commitlint": "^17.7.1",
    "@cub-admin/core": "workspace:^",
    "@cub-admin/utils": "workspace:^",
    "@element-plus/icons-vue": "^2.1.0",
    "@rushstack/eslint-patch": "^1.4.0",
    "@types/color": "^3.0.4",
    "@types/lodash-es": "^4.17.9",
    "@types/path-browserify": "^1.0.0",
    "@unocss/reset": "^0.53.6",
    "@vitejs/plugin-vue": "^4.3.4",
    "@vue/eslint-config-airbnb-with-typescript": "^7.0.0",
    "commitizen": "^4.3.0",
    "element-plus": "^2.3.14",
    "husky": "^8.0.3",
    "lint-staged": "^13.3.0",
    "path-browserify": "^1.0.1",
    "sass": "^1.68.0",
    "typescript": "^5.2.2",
    "unocss": "^0.53.6",
    "unplugin-icons": "^0.17.1",
    "vite": "^4.4.9",
    "vite-plugin-dts": "^3.5.4",
    "vite-svg-loader": "^4.0.0",
    "vitepress": "1.0.0-rc.13",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4",
    "vue-tsc": "^1.8.13"
  },
  "peerDependencies": {
    "element-plus": "^2.3.8",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4"
  },
  "lint-staged": {
    "{packages,preview}/**/*.{vue,ts,tsx,js,cjs,mjs}": "eslint --fix"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  }
}
